<header>
    <%= link_to 'Music App', 'http://localhost:3000/bands', class: 'main-logo' %>

    <ul class="nav_links" id="page-links"><%= yield :links %></ul>

    <ul class="nav_links">
      <% if logged_in? %>
        <li><p><%= current_user.email %></p></li>
        <li><%= button_to "Logout", session_url, method: :delete, data: { confirm: 'Are you sure?' } %></li>
      <% else %>
        <li><%= button_to "Sign In", new_session_url, method: :get %></li>
        <li><%= button_to "Sign Up", new_user_url, method: :get %></li>
      <% end %>
    </ul>
  
</header>